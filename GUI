local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("SoloHub", "Midnight")

--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("AutoTap")

MainSection:NewToggle("AutoTap", "turn AutoTap On or OFF", function(state)
    if state then
        _G.AutoTap = true

while _G.AutoTap == true do 

    game:GetService("ReplicatedStorage").Remotes.Tap:FireServer()
wait()
end
    else
        _G.AutoTap = false

        while _G.AutoTap == true do 
        
            game:GetService("ReplicatedStorage").Remotes.Tap:FireServer()
        wait()
        end
    end
end)
local MainSection = Main:NewSection("AutoRebirth")
MainSection:NewToggle("AutoRebirth", "Turns AutoRebirth ON Or OFF", function(state)
    if state then
        _G.AutoRebirth = true
        
        while _G.AutoRebirth == true do
      

local args = {
    [1] = 1
}

game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer(unpack(args))
wait()
end
    else
    
        _G.AutoRebirth = false
        
        while _G.AutoRebirth == true do
      

local args = {
    [1] = 1
}

game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer(unpack(args))
wait()
    end
end
end)
local MainSection = Main:NewSection("2kRebirths")
MainSection:NewToggle("Auto 2k Rebirths", "", function(state)
    if state then
_G.Auto2kRebirths = true
while _G.Auto2kRebirths == true do
    -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = 6
}

game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer(unpack(args))
wait()
end
    else
        _G.Auto2kRebirths = false
while _G.Auto2kRebirths == true do
    -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = 6
}

game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer(unpack(args))
wait()
end
    end
end)
--Eggs + Hatching
local Eggs = Window:NewTab("Eggs")
local Eggs = Eggs:NewSection("Eggs")
Eggs:NewButton("Hatch 1 Common", "", function()

local args = {
    [1] = "Basic Egg",
    [2] = 1
}

game:GetService("ReplicatedStorage").Remotes.BuyEgg:InvokeServer(unpack(args))

end)
